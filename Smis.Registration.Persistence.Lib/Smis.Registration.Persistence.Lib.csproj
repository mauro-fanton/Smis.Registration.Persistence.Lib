<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RepositoryUrl>https://github.com/mauro-fanton/Smis.Registration.Persistence.Lib</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Smis.Registration.Persistence.Lib</PackageId>
    <Version>1.0.0</Version>
    <Authors>Mauro Fanton</Authors>
    <Product>Smis Registration Persistence Library</Product>
    <PackageTags>Smis;Smis.Refistration.Persistence</PackageTags>
    <Description>
      This client library provide persistence classes for the Smis Registration services for Mongodb 
    </Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Bson" Version="2.23.1" />
    <PackageReference Include="Mongo.Migration" Version="3.1.4" />
  </ItemGroup>
</Project>
